  </select>
  <select id="selectByExampleR" resultMap="BaseResultMap" parameterType="com.city3d.dao.entry.BuildingRemove">
     SELECT b.*, a.* 
	 FROM building_remove b left join address a on b.add_id = a.add_id 
	 where COALESCE(b.remove_name,'') like concat('%',#{removeName},'%') 
	 and COALESCE(b.remove_site,'') like concat('%',#{removeSite},'%') 
	 and COALESCE(b.remove_house_num,'') like concat('%',#{removeHouseNum},'%') 
	 and COALESCE(b.remove_land_num,'') like concat('%',#{removeLandNum},'%') 
	 and COALESCE(b.remove_sign,'') like concat('%',#{removeSign},'%') 
  </select>
