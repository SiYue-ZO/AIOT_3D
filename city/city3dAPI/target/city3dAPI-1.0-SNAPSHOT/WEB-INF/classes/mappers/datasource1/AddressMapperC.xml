  <select id="getAddressAll" resultMap="BaseResultMap" parameterType="java.lang.String">
    select a.*, n.* from address a, net n where a.pid = n.net_id and a.add_type = #{addType} order by a.add_date desc
  </select>
  <select id="queryAddressByPolygon" resultMap="BaseResultMap" parameterType="com.city3d.dao.entry.Address">
 				select B.roomcode from (select *, regexp_split_to_array(t.poi,',') as AAA from address t ) B
		where ST_Contains(ST_PolygonFromText('POLYGON((${poi}))'),(ST_PointFromText('POINT('||AAA[1]||' '||AAA[2]||')')))
  </select>
  <delete id="batchDelete" parameterType="java.util.List" >
